{
  "name": "ultra-pos",
  "version": "2.0.4",
  "description": "Professional ULTRA_POS System using Electron v22",
  "main": "src/main.js",
  "author": "ULTRA_POS Developer",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:win64": "electron-builder --win --x64",
    "build:win32": "electron-builder --win --ia32",
    "build:portable": "electron-builder --win portable",
    "dist": "npm run build",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^9.4.3",
    "canvas": "^3.2.1",
    "fs-extra": "^11.2.0",
    "jimp": "^1.6.0",
    "node-thermal-printer": "^4.5.0",
    "text-to-image": "^8.0.1"
  },
  "devDependencies": {
    "electron": "^22.3.27",
    "electron-builder": "^24.13.3"
  }
}